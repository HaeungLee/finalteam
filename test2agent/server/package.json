  "version": "0.1.0",
  "description": "Template repository of Agentica for NodeJS WebSocket Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "prepare": "ts-patch install && ts-node build/env.ts",
